SELECT I.DESCRIPTION, IL.NAME
FROM ISSUESS I, ISSUESLISTS IL
WHERE I.ISSUESLIST_ID = IL.ID;

SELECT U.FIRSTNAME, U.LASTNAME, I.DESCRIPTION
FROM USERS U, ISSUESS I
WHERE I.USER_ID_ASSIGNEDTO = U.ID;

SELECT U.FIRSTNAME, U.LASTNAME, COUNT(*) AS TASKS_NUMBER
FROM USERS U, ISSUESS I
WHERE I.USER_ID_ASSIGNEDTO = U.ID
GROUP BY U.ID;